@if (TempData["success"] != null)
{
    <script type="text/javascript">
        $(document).ready(function () {
            toastr.success('@TempData["success"]');
        });
    </script>
}
else if (TempData["error"] != null)
{
    <script type="text/javascript">
        $(document).ready(function () {
            toastr.error('@TempData["error"]');
        });
    </script>
}

